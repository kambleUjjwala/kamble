}
.hex {
    color: #72BFFF;
    height: 255px;
    width: 157px;
    display: inline-flex;
    border-radius: 12px!important;
    position: relative;
    background-color: #72BFFF;
    
}





.hex:before,
.hex:after {
  content: '';
  height: 100%;
  width: 100%;
  display: inline-block;
  position: absolute;
  left: 0px;
  top: 0px;
  transform-origin: center center;
  background-color: inherit;
  border-top: inherit;
  border-bottom: inherit;
  box-sizing: inherit;
  border-radius: inherit;
  box-shadow: inherit;
}

.hex:before {
  transform: rotateZ(60deg);
  z-index: 10;
}

.hex:after {
  transform: rotateZ(-60deg);
  z-index: 15;
}
