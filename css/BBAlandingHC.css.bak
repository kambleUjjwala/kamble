.timeline > h3 {
  margin-bottom: 7%;
  font-size: 34px;
  
 text-align: center;
    font-family: 'Quicksand', sans-serif;
}

.timeline > .column {
  display: flex;
  flex-direction: column;
  position: relative;
  margin: 1rem;
}

.timeline > .column::before {
    content: "";
    display: block;
    position: absolute;
    left: 50%;
    transform: translatex(-50%);
    border: 2px solid #1C98FF;
    height: calc(88% / 5 * 4);
    z-index: -1;
	top: 12%;
}

.timeline > .column > .row {
  display: flex;
  
    margin-bottom: 8%;

}
.leftsiide {
  flex: 1 1 calc(50% - 50px);
  position: relative;
     padding: 1% 10% 1% 0%;
}


.timeline  h6 {
     padding: 1rem;
    color: black;
    font-size: 14px;
    font-weight: bold;
}

.timeline > .column > .row p {
     padding: 1rem;
    color: black;
    font-weight: 400;
    margin-top: -2%;
}

/* row icon */
.timeline > .column > .row > .icon {
  flex: 1 1 100px;
}

.timeline > .column > .row > .icon > div {
 
    width: 50px;
    height: 50px;
    border-radius: 50%;
    border: 6px solid #006BC4;
    margin: auto;
    background: white;
    box-shadow: 0 1rem 3rem rgb(0 0 0 / 18%);
    position: relative;
	    top: 46%;

}

.timeline > .column > .row > div i {
  display: block;
  font-size: 2.5rem;
  color: #006BC4;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

/* row time */





.rightside
    {
	flex: 1 1 calc(50% - 50px);
	padding: 1% 0% 1% 10%;
	}

.timeline > .column > .row:nth-child(even) {
  flex-direction: row-reverse;
}



